function arena1_init()
	for x=0, 127 do
	 make_tile(x, 0, 2)
	 make_tile(x, 127, 2)
	end
 
 for y=0, 127 do
  make_tile(0, y, 2)
  make_tile(127, y, 2)
 end
 
 make_tile(10, 64, 0)
 make_tile(50, 64, 1)
end

function arena1_spawn(turn)
 
end

function most_bots_wins()
 if #team0 > #team1 then
  return "team 0 wins!"
 elseif #team1 > #team0 then
  return "team 1 wins!"
 else
  return "tie game."
 end
end
