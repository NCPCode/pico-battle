end
-->8
-- team 1

function update1(x, y)
 robot = {
  dirc = 0,
  instr = 1,
 }
 
 if wget(x - 1, y) == 0
  or wget(x - 1, y - 1) == 0
  then
  
  robot.instr = 0
 end
 
 return robot
end
